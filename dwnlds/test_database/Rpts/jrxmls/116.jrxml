		<parameter name="P_INVOICE_ID" class="java.lang.Long">
			<parameterDescription><![CDATA[Invoice ID:]]></parameterDescription>
		</parameter>
		<parameter name="P_INVOICE_TYPE" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT a.rcvbl_smmry_id, a.rcvbl_smmry_desc, 
a.rcvbl_smmry_amnt, a.code_id_behind, substr(a.rcvbl_smmry_type,2), 
a.auto_calc , REPLACE(REPLACE(a.rcvbl_smmry_type,'2Tax','3Tax'),'3Discount','2Discount') smtyp 
FROM accb.accb_rcvbl_amnt_smmrys a 
WHERE((a.src_rcvbl_hdr_id = $P{P_INVOICE_ID}) 
	and (a.src_rcvbl_type=$P{P_INVOICE_TYPE}) 
	and (substr(a.rcvbl_smmry_type,1,1) IN ('6','7','8'))) 
       ORDER BY 7]]>
		</queryString>
		<field name="rcvbl_smmry_id" class="java.lang.Long"/>
		<field name="rcvbl_smmry_desc" class="java.lang.String"/>
		<field name="rcvbl_smmry_amnt" class="java.math.BigDecimal"/>
		<field name="code_id_behind" class="java.lang.Integer"/>
		<field name="substr" class="java.lang.String"/>
